# Tests for ObjectMacro.

Macro a
    Param
        comment1 : String;
        comment2 : String;
{Begin}
    {comment1} {comment2}
{End}

Macro b
    Param
        comment1 : String;
        comment2 : String;
{Begin}
    {comment1} {comment2}
{End}

Macro macro_internals
    Internal
        comment1 : b;
        comment2 : a;
{Begin}
    {comment1} {comment2}
{End}

Macro macro_internals_references
    Param
        a : String;
        b : b;
        c : a;
        d : macro_internals(b, "{c}"); #incorrect argument type macro expected string found

{Begin}
    {a}
    {b}
    {c}
    {d}
{End}
